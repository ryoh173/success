<div class="container">
<div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
<p><%= notice %></p>

<h5>つぶやき一覧</h5>

<% @tweets.each do |tweet| %>
  <p class = "box" ><%= tweet.content %></p>
  <%= link_to "つぶやきを編集する", edit_tweet_path(tweet.id), class: 'btn btn-default' %>
  <%= link_to 'つぶやきを削除する', tweet_path(tweet.id), data: { confirm: '本当に削除していいですか？'}, method: :delete, class: 'btn btn-default' %>
<% end %>

</div>
</div>